<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>adminlogin</managed-bean-name>
        <managed-bean-class>managedBean.AdminLoginBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>adminreg</managed-bean-name>
        <managed-bean-class>managedBean.AdminRegBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/adminregister.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome> 
            <to-view-id>/adminlogin.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case> 
            <from-outcome>failure</from-outcome> 
            <to-view-id>/adminregister.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>loginbean</managed-bean-name>
        <managed-bean-class>managedBean.StuLoginBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>registerbean</managed-bean-name>
        <managed-bean-class>managedBean.StuRegBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/Stu_Regist.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome> 
            <to-view-id>/Stu_login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case> 
            <from-outcome>failure</from-outcome> 
            <to-view-id>/Stu_Regist.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
